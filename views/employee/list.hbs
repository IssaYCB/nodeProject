                    </div>
                </div>
          </div>    
{{!-- <h3><a class="btn btn-secondary" href="/employee"><i class="fa fa-plus"></i> Create New</a> List</h3> --}}
<div id="innerwrapper">
<table class="table table-striped">
    <thead>
        <tr>
            <th>name</th>
            <th>Category</th>
            <th>Word</th>
            <th>Definition</th>
            <th>Source</th>
            <th>Reference Material</th>
        </tr>
    </thead>
    <tbody>
        {{#each list}}
        <tr>
            <td>{{this.myName}}</td>
            <td>{{this.category}}</td>
            <td>{{this.myWord}}</td>
            <td>{{this.myDefinition}}</td>
            <td>{{this.mySource}}</td>
            <td>{{this.myReferenceMaterials}}</td>
            <td>
                <a href="/employee/{{this._id}}"><i class="fa fa-pencil fa-lg" aria-hidden="true"></i></a>
                <a href="/employee/delete/{{this._id}}" onclick="return confirm('Are you sure to delete this record ?');"><i class="fa fa-trash fa-lg" aria-hidden="true"></i></a>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>
</div> <!--end wrapper-->
</body>